{
  "name": "json_table2tree",
  "version": "0.0.1",
  "description": "A little utility class to transform tabular json structures into a nested tree",
  "scripts": {
    "test": "npm run lint && npm run cover",
    "test:only": "mocha --require babel-core/register --require babel-polyfill --recursive",
    "cover": "nyc --check-coverage npm run test:only",
    "lint": "eslint src test"
  },
  "private": true,
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aljoshakoecher/table2tree.git"
  },
  "keywords": [
    "json",
    "table",
    "tree",
    "table2tree",
    "javascript"
  ],
  "author": "Aljosha Koecher",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aljoshakoecher/table2tree/issues"
  },
  "homepage": "https://github.com/aljoshakoecher/table2tree#readme",
  "dependencies": {
    "json-groupby": "^1.1.0",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.5.0",
    "chai": "^4.1.2",
    "cross-env": "^5.1.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "mocha": "^6.1.3",
    "nyc": "^13.3.0",
    "rimraf": "^2.6.2"
  }
}
